					tab[nbs] = new NupletInt(r.size() + s_new.size() -1);
					tab[nbs] = joinTwoNuplet(r, s_new, att2);
					nbs++;
					s_new_count ++;
					s_new = hasNext(s_new, t2, s_new_count);
				}
				
				
				r_count ++;
				r = hasNext(r, t1, r_count);
				s_count ++;
				s = hasNext(s, t2, s_count);
			}
		}
