package phuongtien;

public class xedap extends xe {
	public int banhxe;
	public int speed;
	public String tenxe;

	public xedap( int banhxe,  int speed,String tenxe){
		super(banhxe,speed);
	}
	public void SoBanhXe(){
		System.out.println("So banh xe cua xe dap la: "+banhxe);
	}
	public void speed(){
		System.out.println("Toc do cua xe dap la: "+speed);
	}
}
